{
  "name": "cvr-data-update",
  "engines": {
    "node": "=8.10.0"
  },
  "version": "0.1.0",
  "description": "CVR lambda triggered by upload to S3 bucket. Used to download csv file from S3 bucket, parse downloaded data and insert it into the database",
  "main": "src/lambda.js",
  "scripts": {
    "test": "export APP_NAME=test && mocha \"src/test/**/*.test.js\" --exit",
    "lint": "eslint .",
    "build": "npm install && npm run lint && npm test && npm run prod",
    "prod": "npm prune --production && npm install --production && zip -r ../cvr-data-update.zip * --exclude /src/test"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.1",
    "aws-sdk": "^2.338.0",
    "cvr-common": "file:../common",
    "iconv-lite": "^0.4.24",
    "json-2-csv": "^2.4.0",
    "lodash": "^4.17.11",
    "promise-sequential": "^1.1.1"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "sinon": "^7.1.1"
  }
}
